<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <artifactId>dlms</artifactId>
    <name>dlms</name>
    <packaging>jar</packaging>

    <parent>
        <groupId>com.energyict</groupId>
        <artifactId>protocols</artifactId>
        <version>9.1-FEAT04.0-SNAPSHOT</version>
    </parent>

    <dependencies>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdw</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdwimpl</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>protocol-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>connectiontypes</artifactId>
            <version>${project.version}</version>
            <scope>provided</scope>
            <exclusions>        <!--Only connectiontypes-->
                <exclusion>
                    <artifactId>*</artifactId>
                    <groupId>*</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>protocolcommon</artifactId>
            <version>${project.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
        </dependency>
        <dependency>
            <groupId>org.powermock</groupId>
            <artifactId>powermock-module-junit4</artifactId>
        </dependency>
        <dependency>
            <groupId>org.powermock</groupId>
            <artifactId>powermock-api-mockito</artifactId>
        </dependency>

        <dependency>
			<groupId>org.easytesting</groupId>
			<artifactId>fest-assert-core</artifactId>
		</dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>
