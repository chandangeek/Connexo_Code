<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.energyict</groupId>
        <artifactId>mdc.parent</artifactId>
        <version>9.1.1-Kratos-SNAPSHOT</version>

    </parent>

    <artifactId>mdc.test.api.rest</artifactId>
    <packaging>bundle</packaging>
    <name>mdc-test-api-rest</name>
    <description>
        Provides extra REST resources to access INTERNAL functionality of the application for testing purposes only
    </description>

    <dependencies>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>org.osgi.core</artifactId>
        </dependency>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>org.osgi.compendium</artifactId>
        </dependency>
        <dependency>
            <groupId>javax.ws.rs</groupId>
            <artifactId>javax.ws.rs-api</artifactId>
        </dependency>
        <dependency>
            <groupId>javax.annotation</groupId>
            <artifactId>javax.annotation-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>rest.util</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>util</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.tou.campaign</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>calendar</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>domain.util</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.upl</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.common</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>pki</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.device.data</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.device.config</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.firmware</artifactId>
        </dependency>
        <dependency>
            <groupId>com.elster.jupiter</groupId>
            <artifactId>orm</artifactId>
        </dependency>
        <dependency>
            <groupId>com.energyict</groupId>
            <artifactId>mdc.protocols.api</artifactId>
        </dependency>
    </dependencies>
    <build>
      <pluginManagement>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-pmd-plugin</artifactId>
            <version>${pmd.version}</version>
            <configuration>
              <skip>true</skip>
            </configuration>
          </plugin>
        </plugins>
      </pluginManagement>
    </build>
</project>
